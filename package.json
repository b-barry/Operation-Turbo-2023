    "@nuxt/image-edge": "^1.0.0-28020728.5df24eb",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@uniformdev/canvas": "^18.34.0",
    "@uniformdev/canvas-cloudinary": "^18.29.0",
    "@uniformdev/canvas-vue": "^18.34.0",
    "@uniformdev/cli": "^18.34.0",
    "@uniformdev/context": "^18.34.0",
    "@uniformdev/context-vue": "^18.34.0",
    "@uniformdev/project-map": "^18.34.0",
    "@uniformdev/uniform-nuxt": "^18.34.0",
    "cloudinary-build-url": "^0.2.4",
    "contentful": "^9.3.5",
    "nuxt": "^3.4.2"
  },
  "devDependencies": {
    "@nuxtjs/plausible": "^0.2.0",
    "@nuxtjs/tailwindcss": "^5.3.3",
    "@types/node": "^18",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "seedrandom": "^3.0.5",
    "traverse": "^0.6.7"
  }
